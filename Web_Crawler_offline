                <script>
              // VIDEO MODAL SCRIPT
              $(window).on('load', function() {
                $(".ribbon").addClass("ribbon--homepage");
                function autoPlayYouTubeModal() {
                  var trigger = $("body").find('[data-toggle="modal"]', '');
              
                  trigger.click(function() {
                    var theModal = $(this).data("target"),
                      videoTitle = encodeURIComponent(
                        $(this).data("title") + " - via @YoungCapital"
                      ),
                      videoTitleTwitter = encodeURIComponent($(this).data("title") + " -"),
                      videoSRC = encodeURIComponent($(this).data("youtubeVideo")),
                      videoText = encodeURIComponent($(this).data("videoText")),
                      videoSRCauto = videoSRC + "?autoplay=1",
                      shareVideoSRC = videoSRC
                      // .replace(
                      //   "www.youtube.com%2Fembed%2F",
                      //   "youtu.be/"
                      // );
              
                    $(theModal + " iframe").attr("src", decodeURIComponent(videoSRCauto));
                    $(".videoText").html(decodeURIComponent(videoText));
              
                    $("#videoModal").on("hidden.bs.modal", function() {
                      $(this)
                        .find("iframe")
                        .attr("src", "");
                      $(".modal-title").html("");
                      $(".videoText").html("");
                    });
                  });
                }
              
                autoPlayYouTubeModal();
                
                // Read-more function which takes 2 elements and extends the first
                $(".read--extender").click(function() {
                //   Define here the element which you want to target with "Read more" and add to the short text
                  var targetElement = $($(this).data('readMoreTarget')),
                      extender = $(this);
                      
                  var extenderIcon = $(this).siblings(".read--extender-icon");
                  
                //   Function which animates the read more and selects the class which need to be extended
                  targetElement.slideUp(0, function() {
                    $(".read--less").append(targetElement);
                    extender.hide(0, function() {
                      targetElement.slideDown(200);
                    });
                    extenderIcon.hide();
                  }); 
                })

              });

